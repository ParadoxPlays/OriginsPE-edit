{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "r4isen1920_originspe:experience_touch",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particle/particles"
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.lifetime = 3.0;variable.size = math.random(0.1,0.2)/variable.lifetime;variable.sphere_size = 0.5;"
			},
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": true
			},
			"minecraft:emitter_rate_instant": {
				"num_particles": 7
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_sphere": {
				"offset": [0, "variable.sphere_size*2", 0],
				"radius": "variable.sphere_size",
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "variable.particle_random_1*variable.lifetime"
			},
			"minecraft:particle_initial_speed": 1,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, "-math.random(0.1,1.0)", 0],
				"linear_drag_coefficient": 2
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.size*(variable.particle_random_1*variable.lifetime-variable.particle_age)", "variable.size*(variable.particle_random_1*variable.lifetime-variable.particle_age)"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 128,
					"flipbook": {
						"base_UV": [56, 96],
						"size_UV": [8, 8],
						"step_UV": [-8, 0],
						"frames_per_second": 1,
						"max_frame": 8,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 0.1,
				"coefficient_of_restitution": 0.1,
				"collision_radius": 0.01
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "variable.particle_age",
					"gradient": {
						"0.0": "#FFE47A00",
						"1.0": "#FF6CFF01"
					}
				}
			}
		}
	}
