    //* Resource bar 3
    "controller.animation.r4isen1920_originspe.resource_bar.bar_3": {
      "initial_state": "init",
      "states": {

        "init": {
          "transitions": [ { "reset_bar": "q.scoreboard('gui') == 1" } ]
        },
