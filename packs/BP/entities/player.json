{
  "format_version": "1.18.20",
  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:player",
      "is_spawnable": false,
      "is_summonable": false,
      "is_experimental": false,

      "properties": {
        "r4isen1920_originspe:toggle_particles": {
          "client_sync": true,
          "type": "bool",
          "default": true
        }
      }
    },

    "component_groups": {

      /* OriginsPE */

      "r4isen1920_originspe:movement.0.05": {
        "minecraft:movement": { "value": 0.05 }
      },
      "r4isen1920_originspe:movement.0.1": {
        "minecraft:movement": { "value": 0.1 }
      },
      "r4isen1920_originspe:movement.0.15": {
        "minecraft:movement": { "value": 0.15 }
      },


      "r4isen1920_originspe:health.40": {
        "minecraft:health": { "max": 40 }
      },
      "r4isen1920_originspe:health.20": {
        "minecraft:health": { "max": 20 }
      },
      "r4isen1920_originspe:health.14": {
        "minecraft:health": { "max": 14 }
      },
      "r4isen1920_originspe:health.10": {
        "minecraft:health": { "max": 10 }
      },


      "r4isen1920_originspe:attack.0": {
        "minecraft:attack": { "damage": 0 }
      },
      "r4isen1920_originspe:attack.1": {
        "minecraft:attack": { "damage": 1 }
      },
      "r4isen1920_originspe:attack.2": {
        "minecraft:attack": { "damage": 2 }
      },
      "r4isen1920_originspe:attack.5": {
        "minecraft:attack": { "damage": 5 }
      },
      "r4isen1920_originspe:attack.7": {
        "minecraft:attack": { "damage": 7 }
      },
      "r4isen1920_originspe:attack.10": {
        "minecraft:attack": { "damage": 10 }
      },


      /* Vanilla */

      "minecraft:add_bad_omen": {
        "minecraft:spell_effects": {
          "add_effects": [
            {
              "effect": "bad_omen",
              "duration": 6000,
              "visible": false,
              "display_on_screen_animation": true
            }
          ]
        },
        "minecraft:timer": {
          "time": [ 0.0, 0.0 ],
          "looping": false,
          "time_down_event": {
            "event": "minecraft:clear_add_bad_omen",
            "target": "self"
          }
        }
      },
      "minecraft:clear_bad_omen_spell_effect": {
        "minecraft:spell_effects": {
        }
      },
      "minecraft:raid_trigger": {
        "minecraft:raid_trigger": {
          "triggered_event": {
            "event": "minecraft:remove_raid_trigger",
            "target": "self"
          }
        },
        "minecraft:spell_effects": {
          "remove_effects": "bad_omen"
        }
      }

    },

    "components": {

      /* OriginsPE */

      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "all",
            "on_damage": {
              "filters": {
                "any_of": [
                  { "test": "has_tag", "value": "_master_of_webs_0" },
                  { "test": "has_tag", "value": "_climbing" }
                ]
              }
            },
            "deals_damage": true,
            "damage_multiplier": 0.5
          },
          {
            "cause": "all",
            "on_damage": {
              "filters": {
                "any_of": [
                  { "test": "has_tag", "value": "_master_of_webs_1" }

                ]
              }
            },
            "deals_damage": true,
            "damage_multiplier": 0.25
          },
          {
            "cause": "all",
            "on_damage": {
              "filters": {
                "all_of": [
                  { "test": "has_tag", "value": "power_more_kinetic_damage" },
                  {
                    "any_of": [
                      { "test": "has_damage", "value": "fall" },
                      { "test": "has_damage", "value": "fly_into_wall" }
                    ]
                  }
                ]
              }
            },
            "deals_damage": true,
            "damage_multiplier": 2
          }
        ]
      },
      "minecraft:environment_sensor": {
        "triggers": [

          /* OriginsPE */

          {
            "filters": {
              "all_of": [
                { "test": "has_tag", "value": "power_shulk_inventory" },
                { "test": "has_tag", "value": "_shulk_inventory_open" },
                { "test": "has_tag", "operator": "!=", "value": "_shulk_inventory_opened" },
                { "test": "has_container_open", "value": true }
              ]
            },
            "event": "r4isen1920_originspe:power.shulk_inventory.open_test"
          },
          {
            "filters": {
              "all_of": [
                { "test": "has_tag", "value": "power_shulk_inventory" },
                { "test": "has_tag", "value": "_shulk_inventory_open" },
                {
                  "any_of": [
                    {
                      "all_of": [
                        { "test": "has_tag", "value": "_shulk_inventory_opened" },
                        { "test": "has_container_open", "value": false }
                      ]
                    },
                    { "test": "is_riding", "value": false },
                    { "test": "is_moving", "value": true },
                    { "test": "is_sneaking", "value": true }
                  ]
                }
              ]
            },
            "event": "r4isen1920_originspe:power.shulk_inventory.close"
          },


          /* Vanilla */

          {
            "filters": {
              "all_of": [
                {
                  "test": "has_mob_effect",
                  "subject": "self",
                  "value": "bad_omen"
                },
                {
                  "test": "is_in_village",
                  "subject": "self",
                  "value": true
                }
              ]
            },
            "event": "minecraft:trigger_raid"
          }
        ]
      },

      /* Vanilla */

      "minecraft:experience_reward": {
        "on_death": "Math.Min(query.player_level * 7, 100)"
      },
      "minecraft:type_family": {
        "family": [ "player" ]
      },
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:loot": {
        "table": "loot_tables/empty.json"
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.8
      },
      "minecraft:can_climb": {
      },
      "minecraft:movement": {
        "value": 0.1
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:attack": {
        "damage": 1
      },
      "minecraft:exhaustion_values": {
        "heal": 6,
        "jump": 0.05,
        "sprint_jump": 0.2,
        "mine": 0.005,
        "attack": 0.1,
        "damage": 0.1,
        "walk": 0.0,
        "sprint": 0.1,
        "swim": 0.01
      },
      "minecraft:player.saturation": {
        "value": 5,
         "max": 20
      },
      "minecraft:player.exhaustion": {
        "value": 0,
        "max": 20
      },
      "minecraft:player.level": {
        "value": 0,
        "max": 24791
      },
      "minecraft:player.experience": {
        "value": 0,
        "max": 1
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": -1,
        "inhale_time": 3.75,
        "generates_bubbles": false
      },
      "minecraft:nameable": {
        "always_show": true,
        "allow_name_tag_renaming": false
      },
      "minecraft:physics": {
        "push_towards_closest_space": true
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:insomnia": {
        "days_until_insomnia": 3
      },
      "minecraft:rideable": {
        "seat_count": 2,
        "family_types": [
          "parrot_tame"
        ],
        "pull_in_entities": true,
        "seats": [
          {
            "position": [ 0.4, -0.2, -0.1 ],
            "min_rider_count": 0,
            "max_rider_count": 0,
            "lock_rider_rotation": 0
          },
          {
            "position": [ -0.4, -0.2, -0.1 ],
            "min_rider_count": 1,
            "max_rider_count": 2,
            "lock_rider_rotation": 0
          }
        ]
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:block_climber": {}
    },

    "events": {

      /* OriginsPE */

      "r4isen1920_originspe:movement.0.05": {
        "add": { "component_groups": [ "r4isen1920_originspe:movement.0.05" ] }
      },
      "r4isen1920_originspe:movement.0.1": {
        "add": { "component_groups": [ "r4isen1920_originspe:movement.0.1" ] }
      },
      "r4isen1920_originspe:movement.0.15": {
        "add": { "component_groups": [ "r4isen1920_originspe:movement.0.15" ] }
      },

      "r4isen1920_originspe:health.40": {
        "add": { "component_groups": [ "r4isen1920_originspe:health.40" ] }
      },
      "r4isen1920_originspe:health.20": {
        "add": { "component_groups": [ "r4isen1920_originspe:health.20" ] }
      },
      "r4isen1920_originspe:health.14": {
        "add": { "component_groups": [ "r4isen1920_originspe:health.14" ] }
      },
      "r4isen1920_originspe:health.10": {
        "add": { "component_groups": [ "r4isen1920_originspe:health.10" ] }
      },

      "r4isen1920_originspe:attack.0": {
        "add": { "component_groups": [ "r4isen1920_originspe:attack.0" ] }
      },
      "r4isen1920_originspe:attack.1": {
        "add": { "component_groups": [ "r4isen1920_originspe:attack.1" ] }
      },
      "r4isen1920_originspe:attack.2": {
        "add": { "component_groups": [ "r4isen1920_originspe:attack.2" ] }
      },
      "r4isen1920_originspe:attack.5": {
        "add": { "component_groups": [ "r4isen1920_originspe:attack.5" ] }
      },
      "r4isen1920_originspe:attack.7": {
        "add": { "component_groups": [ "r4isen1920_originspe:attack.7" ] }
      },
      "r4isen1920_originspe:attack.10": {
        "add": { "component_groups": [ "r4isen1920_originspe:attack.10" ] }
      },

      "r4isen1920_originspe:options.particle_on": {
        "set_property": { "r4isen1920_originspe:toggle_particles": true }
      }, 
      "r4isen1920_originspe:options.particle_off": {
        "set_property": { "r4isen1920_originspe:toggle_particles": false }
      },

      "r4isen1920_originspe:power.shulk_inventory.open_test": { 
        "queue_command": {
          "command": [ "tag @s add _shulk_inventory_opened" ] 
        }
      },
      "r4isen1920_originspe:power.shulk_inventory.close": { 
        "queue_command": {
          "command": [ "tag @s add _control_use_shulk_inventory" ] 
        } 
      },


      /* Vanilla */

      "minecraft:gain_bad_omen": {
        "add": { "component_groups": [ "minecraft:add_bad_omen" ] }
      },
      "minecraft:clear_add_bad_omen": {
        "remove": { "component_groups": [ "minecraft:add_bad_omen" ] },
        "add": { "component_groups": [ "minecraft:clear_bad_omen_spell_effect" ] }
      },
      "minecraft:trigger_raid": {
        "add": { "component_groups": [ "minecraft:raid_trigger" ] }
      },
      "minecraft:remove_raid_trigger": {
        "remove": { "component_groups": [ "minecraft:raid_trigger" ] }
      }

    }
  }
}