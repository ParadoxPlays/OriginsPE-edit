{
	"format_version": "1.20.30",
	"minecraft:item": {
		"description": {
			"identifier": "origins:inchling_sugar",
			"category": "Commands",
      "is_hidden_in_commands": true
		},

		"components": {
			"minecraft:icon": {
        "texture": "sugar"
      },
      "minecraft:display_name": {
        "value": "item.sugar.name"
      },
			"minecraft:food": {
				"nutrition": 2,
				 
				"can_always_eat": true,
				"on_consume": {
					"event": "origins:on_consume",
					"target": "self"
				}
			},

      "minecraft:max_stack_size": 64,

      "minecraft:stacked_by_data": false,
			"minecraft:use_duration": 1.6,
			"minecraft:use_animation": "eat"
		},

		"events": {
			"origins:on_consume": {
				"condition": "q.scoreboard('trait') == 14",
				"add_mob_effect": {
					"effect": "speed",
					"target": "holder",
					"duration": 20,
					"amplifier": 7
				},
				"run_command": {
					"command": [
						"scoreboard players set @s inchlingspeed 1",
						"playsound ui.enchant @s ~~~" 
					],
					"target": "holder"
				}
			}
		}
	}
}